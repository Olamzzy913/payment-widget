{
  "version": 2,
  "buildCommand": "npm run build:all",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/sdk/(.*)",
      "destination": "/sdk/$1"
    },
    {
      "source": "/iframe/(.*)",
      "destination": "/iframe/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/iframe/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sdk/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/iframe/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        }
      ]
    }
  ]
}
