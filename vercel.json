{
  "version": 2,
  "buildCommand": "npm run build:all",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/sdk/(.*)",
      "destination": "/sdk/$1"
    },
    {
      "source": "/iframe/(.*)",
      "destination": "/iframe/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/iframe/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
